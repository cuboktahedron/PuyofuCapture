<configuration>
  <appSettings>
    <add key="captureRect" value="-1,-1,-1,-1" lockItem="true"/>
    <add key="nextRect" value="-1,-1,-1,-1" lockItem="true"/>
  </appSettings>
  <system.diagnostics>
    <trace autoflush="false" indentsize="4">
      <listeners>
        <remove name="Default" />
        <add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="debug.log" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>